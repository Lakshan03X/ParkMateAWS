{
  "expo": {
    "name": "ParkMate",
    "slug": "parkmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appImages/appstore.png",
    "scheme": "parkmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "ParkMate needs access to your photo library to save payment receipts.",
        "NSPhotoLibraryAddUsageDescription": "ParkMate needs access to save payment receipts to your photo library."
      }
    },
    "android": {
      "ndkVersion": "27.0.12077973",
      "icon": "./assets/appImages/appstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appImages/appstore.png",
        "backgroundColor": "#093F86"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lakshan03x.parkmate"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow ParkMate to save payment receipts to your photo library.",
          "savePhotosPermission": "Allow ParkMate to save payment receipts.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "API_URL": "http://localhost:3000/api",
      "API_KEY": "your-api-key",
      "router": {},
      "eas": {
        "projectId": "666308ec-e28c-4781-8f91-51deb67c5ca6"
      },
      "AWS_REGION": "us-east-1",
      "AWS_USER_POOL_ID": "your-aws-user-pool-id",
      "AWS_USER_POOL_CLIENT_ID": "your-aws-user-pool-client-id",
      "AWS_IDENTITY_POOL_ID": "your-aws-identity-pool-id",
      "AWS_API_GATEWAY_URL": "https://your-api-id.execute-api.us-east-1.amazonaws.com/prod",
      "AWS_S3_BUCKET": "your-s3-bucket-name",
      "MOSIP_BASE_URL": "https://api.mosip.net",
      "MOSIP_CLIENT_ID": "your_mosip_client_id",
      "MOSIP_CLIENT_SECRET": "your_mosip_client_secret",
      "MOSIP_REDIRECT_URI": "your_redirect_uri",
      "MOSIP_AUTH_ENDPOINT": "/v1/authmanager/authenticate",
      "MOSIP_ESIGNET_ENDPOINT": "/v1/esignet",
      "GOOGLE_CLOUD_VISION_API_KEY": "AIzaSyCHfV0XVhkM-HPDkv0Ev3pojPACCY_FyHs",
      "EXPO_PUBLIC_GEMINI_API_KEY": ""
    },
    "owner": "lakshan03xs-organization"
  }
}
