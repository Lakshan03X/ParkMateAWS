{
  "expo": {
    "name": "ParkMate",
    "slug": "parkmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appImages/appstore.png",
    "scheme": "parkmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "ParkMate needs access to your photo library to save payment receipts.",
        "NSPhotoLibraryAddUsageDescription": "ParkMate needs access to save payment receipts to your photo library."
      }
    },
    "android": {
      "ndkVersion": "27.0.12077973",
      "icon": "./assets/appImages/appstore.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/appImages/appstore.png",
        "backgroundColor": "#093F86"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.lakshan03x.parkmate"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow ParkMate to save payment receipts to your photo library.",
          "savePhotosPermission": "Allow ParkMate to save payment receipts.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "API_URL": "${API_URL}",
      "API_KEY": "${API_KEY}",
      "router": {},
      "eas": {
        "projectId": "666308ec-e28c-4781-8f91-51deb67c5ca6"
      },
      "AWS_REGION": "${AWS_REGION}",
      "AWS_USER_POOL_ID": "${AWS_USER_POOL_ID}",
      "AWS_USER_POOL_CLIENT_ID": "${AWS_USER_POOL_CLIENT_ID}",
      "AWS_IDENTITY_POOL_ID": "${AWS_IDENTITY_POOL_ID}",
      "AWS_API_GATEWAY_URL": "${AWS_API_GATEWAY_URL}",
      "AWS_S3_BUCKET": "${AWS_S3_BUCKET}",
      "MOSIP_BASE_URL": "${MOSIP_BASE_URL}",
      "MOSIP_CLIENT_ID": "${MOSIP_CLIENT_ID}",
      "MOSIP_CLIENT_SECRET": "${MOSIP_CLIENT_SECRET}",
      "MOSIP_REDIRECT_URI": "${MOSIP_REDIRECT_URI}",
      "MOSIP_AUTH_ENDPOINT": "${MOSIP_AUTH_ENDPOINT}",
      "MOSIP_ESIGNET_ENDPOINT": "${MOSIP_ESIGNET_ENDPOINT}",
      "GOOGLE_CLOUD_VISION_API_KEY": "${GOOGLE_CLOUD_VISION_API_KEY}",
      "EXPO_PUBLIC_GEMINI_API_KEY": "${EXPO_PUBLIC_GEMINI_API_KEY}"
    },
    "owner": "lakshan03xs-organization"
  }
}
